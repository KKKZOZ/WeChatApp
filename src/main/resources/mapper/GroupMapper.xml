<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kkkzoz.mapper.GroupMapper">

    <select id="findByStudentId" resultType="com.kkkzoz.domain.entity.Group">
        select *
        from class_group
        where student_id = #{userId}
    </select>
</mapper>